{
  "name": "tpescript-express-template",
  "version": "0.1.0",
  "description": "template project for an express application using typescript",
  "main": "build/app.js",
  "scripts": {
    "tsc": "tsc",
    "dev": "ts-node-dev --respawn --transpileOnly ./app/app.ts",
    "prod": "tsc && node ./build/app.js",
    "test": "jasmine-ts --config=jasmine.json"
  },
  "author": "David Glendenning (david.glendenning@zerosandones.dev)",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/zerosandones/typescript-express-template"
  },
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/jasmine": "^3.3.15",
    "jasmine": "^3.4.0",
    "jasmine-ts": "^0.3.0",
    "ts-node-dev": "^1.0.0-pre.40",
    "typescript": "^3.5.3"
  }
}
